    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.1.0",
    "resolve": "1.8.1",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "1.1.1",
